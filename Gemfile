source "https://rubygems.org"

# Jekyll
gem "jekyll"


# If you're using GitHub Pages, you might also include the GitHub-pages gem,
# which bundles a set of approved plugins, themes, and dependencies.
# Uncomment the following line if your site is hosted with GitHub Pages:
gem "github-pages", group: :jekyll_plugins

# Plugins for Jekyll are also included here
group :jekyll_plugins do
  gem "jekyll-feed", "~> 0.12"
end
